<div id="<%= dom_id profile %>">
  <p>
    <strong>User:</strong>
    <%= profile.user_id %>
  </p>

  <p>
    <strong>First name:</strong>
    <%= profile.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= profile.last_name %>
  </p>

  <p>
    <strong>Nickname:</strong>
    <%= profile.nickname %>
  </p>

  <p>
    <strong>Balance:</strong>
    <%= profile.balance %>
  </p>

</div>
